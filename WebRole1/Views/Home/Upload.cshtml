
@{
    ViewBag.Title = "Create New Profile";
}
<div class="row">
    <div class="col-md-4 offset-md-4">
        <h2 class="text-center">New container</h2>
        @using (Html.BeginForm("Upload", "Home", FormMethod.Post, new { enctype = "multipart/form-data" })) {
            <div class="form-group">
                <label for="container">Container's Name</label>
                <input type="text" class="form-control" id="container" name="container" placeholder="Container's Name">
            </div>
            <div class="form-group">
                <input type="file" name="file1" class="form-control-file" id="exampleInputFile" aria-describedby="fileHelp">
            </div>
            <div class="form-group">
                <input type="file" name="file2" class="form-control-file" id="exampleInputFile" aria-describedby="fileHelp">
            </div>
            <button type="submit" class="btn btn-primary btn-block">Upload</button>}
    </div>
</div>




